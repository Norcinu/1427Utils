        <DockPanel LastChildFill="True" Height="30" Margin="9,12,9,0" Name="stpMainButtons" VerticalAlignment="Top" Width="600" HorizontalAlignment="Center" Grid.ColumnSpan="2" />
    </Grid>
</Window>
