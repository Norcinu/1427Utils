        <DockPanel LastChildFill="True" Height="30" Margin="12,12,12,0" Name="stpButtons" VerticalAlignment="Top" Width="600" HorizontalAlignment="Center" />
        <Button Content="Exit" Height="23" HorizontalAlignment="Left" Margin="667,634,0,0" Name="btnExit" VerticalAlignment="Top" Width="75" Click="btnExit_Click" />
        <StackPanel Height="355" HorizontalAlignment="Center" Margin="12,48,12,0" Name="stpMainPanel" VerticalAlignment="Top" Width="600" Background="#FFDBC2C2">
            <Label Height="28" Name="label1" HorizontalContentAlignment="Center" />
            <Label Height="28" Name="label2" HorizontalContentAlignment="Center" />
            <Label Height="28" Name="label3" HorizontalContentAlignment="Center" />
            <Label Height="28" Name="label4" HorizontalContentAlignment="Center" />
            <Label Height="28" Name="label5" HorizontalContentAlignment="Center" />
            <Label Height="28" Name="label6" HorizontalContentAlignment="Center" />
            <Label Height="28" Name="label7" HorizontalContentAlignment="Center" />
            <Label Height="28" Name="label8" HorizontalContentAlignment="Center" />
            <Label Height="28" Name="label9" HorizontalContentAlignment="Center" />
            <Label Height="28" Name="label10" HorizontalContentAlignment="Center" />
            <Button Content="Start Test" Height="23" Name="btnStart" Width="75" />
        </StackPanel>
    </Grid>
</Window>
