        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Test Suite" Height="480" Width="640" Loaded="Window_Loaded" HorizontalAlignment="Center" VerticalAlignment="Center" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
